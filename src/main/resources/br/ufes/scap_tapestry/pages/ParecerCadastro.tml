<html t:type="layout" title="Cadastro Parecer" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd">


	<t:form t:id="cadastro" class="form-horizontal">
		<fieldset>
			<legend>Entre com os Dados do seu Parecer:</legend>
			<div class="control-group">
			  <label class="control-label" for="textinput">Motivo:</label>
			  <div class="controls">
			  	<t:textarea t:id="motivo" placeholder="Digite aqui o Motivo do Ideferimento"/>
			  </div>
			</div>
			<div class="control-group">
			  <t:label for="tipoParecer"/>:
			  <div class="controls">
			    <t:select t:id="tipoParecer"/>
			  </div>
			</div>
			<div class="control-group">
			  <label class="control-label" for="singlebutton"></label>
			  <div class="controls">
			    <t:submit class="btn btn-primary" value="Salvar"/>
			  </div>
			</div>
		</fieldset>
	</t:form>


</html>